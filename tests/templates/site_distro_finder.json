{
    "set": {
        "config_paths": [
            "{relative_root}/configs"
        ],
        "distro_paths": [
            "{relative_root}/distros/*"
        ],
        "downloads": {
            "cache_root": "{relative_root}/downloads",
            "distros": [
                [
                    "hab.distro_finders.distro_finder:DistroFinder",
                    "{{ zip_root }}/*"
                ]
            ],
            "install_root": "{relative_root}/distros"
        }
    }
}
