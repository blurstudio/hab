{
    "append": {
        "distro_paths": [
            "{relative_root}/duplicates/distros_1/*"
        ],
        "platform_path_maps": {
            "network-mount": {
                "linux": "/mnt/work/shared_resources",
                "osx": "/mnt/work/osx/shared_resources",
                "windows": "g:\\work\\shared_resources"
            }
        }
    },
    "set": {
        "generic_value": true,
        "override": "site_override.json",
        "filename": "site_override.json",
        "colorize": false
    }
}
